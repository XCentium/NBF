<div class="row breadcrumb-wrap">
    <ul class="widget-breadcrumb breadcrumbs [[Model.CssClass]]">
        [% for parent in Model.Drop.Parents -%]
        <li class="[[parent.CssClass]]">
            <a href="[[parent.Url]]">
                [[parent.Title]]
            </a>
        </li>
        [% endfor -%]
        <li class="hide-for-small current">
            [[CurrentPage.Title]]
        </li>
    </ul>
    <ul class="share-links">
        <li>Share</li>
        <li class="cell"><a href="#"><i class="fa fa-envelope" aria-hidden="true"></i></a></li>
        <li class="cell"><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
        <li class="cell"><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
        <li class="cell"><a href="#"><i class="fa fa-pinterest-p" aria-hidden="true"></i></a></li>
        <li class="cell"><a href="#" class="ico ico-printer-with-paper" aria-hidden="true"></a></li>
    </ul>
</div>